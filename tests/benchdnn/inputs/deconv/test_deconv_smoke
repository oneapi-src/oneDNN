--reset

--match=.*deconv_ci_2d.* # Use 2d problems only from shapes_ci
--mb=2
--alg=direct
--stag=any
--wtag=any
--dtag=any

# Training
## Forward
--dir=FWD_B
--dt=f32,bf16
--attr-post-ops=,linear:2:1
--batch=shapes_ci
## Backward
--dir=BWD_D,BWD_WB
--attr-post-ops=
--batch=shapes_ci

# Inference
--dir=FWD_I
--attr-post-ops=,linear:2:1

--dt=f16
--batch=shapes_ci

--dt=s8:s8:f32
--attr-zero-points=src:common:1
--attr-scales=src:common:0.25*+wei:per_oc:5*+dst:common:2*
--batch=shapes_ci

--dt=u8:s8:s32
--attr-zero-points=
--attr-scales=src:common:0.25*+wei:per_oc:5*
--batch=shapes_ci
