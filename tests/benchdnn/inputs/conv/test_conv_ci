--reset

--mb=2

# Training
## Forward
--dir=FWD_B,FWD_D
### Direct
--alg=direct
--cfg=f32,bf16bf16bf16,f16
--stag=any,axb
--dtag=any,axb
--attr-post-ops=, \
                sum:0.5, \
                linear:2:1, \
                add:f32, \
                prelu:per_oc, \
                mul:s8:per_oc+sum:0.25+relu:0.5+add:f32:per_tensor
--batch=shapes_basic

## Backward
--dir=BWD_D,BWD_W,BWD_WB
--attr-post-ops=
### Direct
--alg=direct
--cfg=f32,bf16bf16bf16
--stag=any,axb
--dtag=any,axb
--batch=shapes_basic

# Inference
--dir=FWD_I
--attr-scales=,src:common:0.25*,wei:per_oc:5*,dst:common:2*,src:common:0.25*+wei:per_oc:5*

## Direct
--alg=direct

### All inference configs
--cfg=s8s8f32,s8s8bf16,s8s8f16,s8s8s32,s8s8s8,s8s8u8, \
      u8s8f32,u8s8bf16,u8s8f16,u8s8s32,u8s8s8,u8s8u8
--batch=shapes_basic

### Unsigned input
--cfg=u8s8s32
--stag=any,axb
--dtag=any,axb
--attr-post-ops=, \
                sum:0.5, \
                linear:2:1, \
                add:f32, \
                sum:0.5:3+add:f32:per_oc+add:f32:per_tensor
--attr-zero-points=
--batch=shapes_basic
--attr-post-ops=
--attr-zero-points=,src:common:2*+dst:common:1*,src:per_dim_1:1*+dst:per_dim_1:1*
--batch=shapes_basic
### Signed input
--cfg=s8s8s8
--stag=any
--dtag=any
--attr-post-ops=, \
                sum:0.5:0:u8, \
                linear:2:1, \
                add:f32, \
                sum:0.5:2+add:f32:per_oc+add:f32:per_tensor
--attr-zero-points=
--batch=shapes_basic
--attr-post-ops=
--attr-zero-points=,src:common:2*+dst:common:1*,src:per_dim_1:1*+dst:per_dim_1:1*
--batch=shapes_basic
# BF32
--reset
--dir=FWD_D,BWD_D
--cfg=f32
--stag=any
--dtag=any
--attr-fpmath=bf16
--batch=shapes_basic
