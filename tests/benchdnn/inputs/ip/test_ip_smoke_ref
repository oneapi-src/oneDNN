--stag=any
--wtag=any
--dtag=any

# Training
## Forward
--dir=FWD_B
--dt=f32,bf16
--attr-post-ops=,linear:2:1
--impl=ref
--batch=shapes_ci
## Backward
--dir=BWD_D,BWD_WB
--attr-post-ops=
--impl=ref
--batch=shapes_ci

# Inference
--dir=FWD_I
--attr-post-ops=,linear:2:1

--dt=f16,f16:f16:s8,bf16:bf16:f32
--impl=ref
--batch=shapes_ci

--dt=s8:s8:f32,u8:s8:f16
--attr-scales=src:common:0.25+wei:per_oc+dst:common:2
--impl=ref
--batch=shapes_ci

--dt=u8:s8:s32
--attr-scales=src:common:0.25+wei:per_oc
--impl=ref
--batch=shapes_ci
