--reset

--match=.*WnD.* # Use WnD inputs from shapes_ci only
--mb=2
--stag=any
--wtag=any
--dtag=any

# Training
## Forward
--dir=FWD_B
--cfg=f32,bf16bf16bf16
--attr-post-ops=,linear:2:1
--batch=shapes_ci
## Backward
--dir=BWD_D,BWD_WB
--attr-post-ops=
--batch=shapes_ci

# Inference
--dir=FWD_I
--attr-post-ops=,linear:2:1

--cfg=f16
--batch=shapes_ci

--cfg=s8s8f32
--attr-scales=src:common:0.25*+wei:per_oc:5*+dst:common:2*
--batch=shapes_ci

--cfg=u8s8s32
--attr-scales=src:common:0.25*+wei:per_oc:5*
--batch=shapes_ci
