--reset

--mb=2

# Training
## Forward
--dir=FWD_B,FWD_D
--dt=f32,bf16,f16
--stag=any,axb
--dtag=any,axb
--attr-post-ops=, \
                sum:0.5, \
                linear:2:1, \
                add:f32, \
                mul:s8:per_oc+sum:0.25+relu:0.5+add:f32:per_tensor
--batch=shapes_ci
## Backward
--dir=BWD_D,BWD_W,BWD_WB
--attr-post-ops=
--batch=shapes_ci

# Inference
--dir=FWD_B,FWD_I
--stag=any,axb
--dtag=any,axb
--attr-scales=,src:common:0.25*,wei:per_oc:5*,dst:common:0.25*

## All inference configs
--dt=u8:s8:f32,u8:s8:bf16,u8:s8:f16,u8:s8:s32,u8:s8:s8,u8:s8:u8, \
      s8:s8:f32,s8:s8:bf16,s8:s8:f16,s8:s8:s32,s8:s8:s8,s8:s8:u8, \
      f16:f16:s8,f16:f16:u8
--batch=shapes_ci

--dt=u8:s8:f32,s8:s8:s32
--attr-post-ops=, \
                sum:0.5, \
                linear:2:1, \
                add:f32, \
                mul:s8:per_oc+sum:0.25+relu:0.5+add:f32:per_tensor
--batch=shapes_ci
