--attr-zero-points=,src:common:1*,wei:common:2*,src:common:1*+wei:common:2* 1x1:1x1

--stag=ab,ba --wtag=ab,ba --dtag=ab
--attr-zero-points=src:common:1*+wei:common:2* 2x2:2x2

