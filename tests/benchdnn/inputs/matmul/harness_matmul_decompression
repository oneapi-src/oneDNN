--wtag=any,ab,ba
--dt=bf16:s8:bf16,bf16:u8:bf16
--attr-scales=wei:common:2,wei:per_oc:bf16
--attr-fpmath=bf16:true
--batch=shapes_2d

--reset
--skip-impl=ref
