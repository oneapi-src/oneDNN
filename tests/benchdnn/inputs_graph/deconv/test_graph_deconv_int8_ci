# int8 tests - no groups
--reset
--dir=FWD_D
--cfg=u8s8s8,s8s8s8
--attr-scales=src:common:1*+wei:common:1*+dst:common:1*,src:common:5*+wei:common:5*+dst:common:5*
--attr-zero-points=,src:common:0*+dst:common:0*
--batch=shapes_2d

# int8 tests - with groups
--reset
--dir=FWD_D
--cfg=u8s8s8,s8s8s8
--stag=abcd
--wtag=acbde
--dtag=abcd
--attr-scales=src:common:5*+wei:common:5*+dst:common:5*
--batch=shapes_2d_groups

# int8 + post-ops - no groups
--reset
--dir=FWD_D,FWD_B
--cfg=u8s8s8,s8s8s8
--attr-scales=src:common:5*+wei:common:5*
--attr-zero-points=,src:common:2*+dst:common:3*
--attr-post-ops=sum,sum:0.0137735:3,relu,add:s8:common
--batch=shapes_2d

--reset
--dir=FWD_D,FWD_B
--cfg=u8s8s8,s8s8s8
--attr-scales=src:common:5*+wei:common:5*+dst:common:5*
--attr-post-ops=abs,elu,exp,gelu_erf,clip_v2,log,round,logistic,sqrt,square,tanh
g1oc3ic16_oh5ih5kh3ph1_n"2d_conv:1st"

# int8 + post-ops - with groups
--reset
--dir=FWD_D,FWD_B
--cfg=u8s8s8,s8s8s8
--stag=abcd
--wtag=acbde
--dtag=abcd
--attr-scales=src:common:5*+wei:common:5*+dst:common:5*
--attr-zero-points=,src:common:2*+dst:common:3*
--attr-post-ops=sum,sum:0.0137735:3,relu,add:s8:common
--batch=shapes_2d_groups

--reset
--dir=FWD_D,FWD_B
--cfg=u8s8s8,s8s8s8
--stag=abcd
--wtag=acbde
--dtag=abcd
--attr-scales=src:common:5*+wei:common:5*+dst:common:5*
--attr-post-ops=abs,elu,exp,gelu_erf,clip_v2,log,round,logistic,sqrt,square,tanh
g4oc16ic16_oh5ih5kh3ph1_n"2d_conv:grouped"

# int8 + several binary post-ops - no groups
--reset
--cfg=s8s8s8
--dir=FWD_I
--attr-post-ops=add:s8:common+add:s8:common,add:s8:common+mul:f32:per_oc,div:f32:common+add:s8:common+mul:f32:per_oc
mb50ic64ih56oc64oh56kh1ph0

# int8 + several binary post-ops - with groups
--reset
--cfg=s8s8s8
--dir=FWD_I
--stag=abcd
--wtag=acbde
--dtag=abcd
--attr-post-ops=add:s8:common+add:s8:common,add:s8:common+mul:f32:per_oc,div:f32:common+add:s8:common+mul:f32:per_oc
g4oc16ic16_oh5ih5kh3ph1_n"2d_conv:grouped"
